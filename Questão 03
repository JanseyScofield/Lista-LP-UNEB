/* Faça um algoritmo que leia dois valores e, através de uma comparação, escreva o maior deles. Considere que os dois valores são diferentes*/

#include <stdio.h>
#include <locale.h>

main(){
	setlocale(LC_ALL,"");
	int num1,  num2;
	scanf("%d %d",&num1, &num2);
	if(num1>num2){
		printf("O primeiro número é maior.");
	}
	else{
		printf("O segundo número é maior.");
	}
}
